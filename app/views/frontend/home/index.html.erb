
<div class="row newer_content">
  <div class="col-md-5 newer_codes">
    <%= render partial: 'frontend/home/codes', locals: { codes: @codes } %>
  </div>

  <div class="col-md-7 newer_posts">
    <%= render partial: 'frontend/home/posts', locals: { posts: @posts } %>
  </div>
</div>

<script type="text/javascript">
  $(function(){
    $("form.navbar-search").on("submit", function(){
      keyword = $(this).find("input.search-query").val().trim();
      $(this).attr('action', $(this).attr('action') + keyword);
    });
  });
</script>
